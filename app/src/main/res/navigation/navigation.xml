<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/navigation"
        app:startDestination="@id/titleFragment">

    <fragment
            android:id="@+id/titleFragment"
            android:name="com.example.parking.titleFragment"
            android:label="titleFragment" >
        <action
                android:id="@+id/action_titleFragment_to_carParkFragment"
                app:destination="@+id/carParkFragment"
                app:popUpTo="@+id/titleFragment"
                app:popUpToInclusive="false" />
    </fragment>
    <fragment
            android:id="@+id/carParkFragment"
            android:name="com.example.parking.CarParkFragment"
            android:label="CarParkFragment" />
    <fragment
            android:id="@+id/infoFragment"
            android:name="com.example.parking.InfoFragment"
            android:label="InfoFragment" />
</navigation>